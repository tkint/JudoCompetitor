<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fr.competitor">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.article.Articles" />
        <activity android:name=".view.article.ViewArticle" />
        <activity android:name=".view.user.Users" />
        <activity android:name=".view.user.ViewUser" />
        <activity android:name=".view.competition.Competitions" />
        <activity android:name=".view.competition.ViewCompetition" />
        <activity android:name=".view.About" />
        <activity android:name=".view.Connect" />
        <activity android:name=".view.competition.result.Results" />
        <activity android:name=".view.competition.result.ViewResult" />
    </application>

</manifest>